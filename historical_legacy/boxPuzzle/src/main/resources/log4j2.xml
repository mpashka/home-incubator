<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <Appenders>
        <Console name="console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss,SSS} bh %-5p [%10.10t] %40.40c - %m%n"/>
        </Console>

    </Appenders>

    <Loggers>
        <!--<Logger name="com.contextweb" level="debug"/>-->

        <Root level="all">
            <AppenderRef ref="console"/>
        </Root>
    </Loggers>
</Configuration>
