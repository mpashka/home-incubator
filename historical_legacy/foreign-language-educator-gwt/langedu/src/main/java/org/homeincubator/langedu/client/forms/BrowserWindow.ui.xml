<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <div class='browser-window'>
        <div class="browser-window-triangle"></div>
        <div ui:field="browserBookmarks" style="position: relative; width: 100%; height: 16px;"></div>
        <input ui:field="browserAddress" class='browser-address' />
        <iframe ui:field="browserContent" class='browser-content' scrolling='yes' src='https://www.google.ru'/>
    </div>
</ui:UiBinder>
